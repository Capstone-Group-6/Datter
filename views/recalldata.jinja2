{% extends "base.jinja2" %}
{% block title %}{{ super() }} - Dataset Listing{% endblock %}
{% block content %}

<h1>Recall Data</h1>
<ul class = "navbarbackground">
	<li>
		<div class="navbar"><a href="/">Home</a></div>
	</li>
	<li>		<div class="navbar"><a href="/logout">Login</a></div>
	</li>
	<li>
		<div class="navbar"><a href="#">Recall Data</a></div>
	</li>
	<li>
		<div class="navbar"><a href="/help">Help</a></div>
	</li>
	<li>
		<div class="company"><a href="/">Datter</a></div>
	</li>
	{% if username %}<li>
		<div class="company"><a href="/logout">Logout</a></div>
	</li>{% endif %}
</ul>
<br>
<div class="recall">
<label for="username">Username: </label>
    <input type="test" id="username" name="username">
    <br>
    <br>
    <label for="testrun">Test run: </label>
    <center><input list="testtype" id="testrun" name="testrun">
    <datalist id="testtype">
        <option value="fat">
        <option value="ash">
        <option value="moisture">
        <option value="protein">
    </datalist>
    <br>
    <br>

    
  <div class="input_one">
   <label class="date_name" for="FirstTest">First Test Date: </label>
    <input type="date" id="FirstTest" type="date" name="FirstTest" readonly onclick="calender(this)">
    </div>
   
    <br>
    <br>
    <div class="input_two">
   <label class="date_name" for="FinalTest">Final Test Date: </label>
    <input type="date" id="FinalTest" type="date" name="FinalTest" readonly onclick="calender(this)">
    </div>
    <br>
    <br>
    <input class="recall-log" type="submit" value="Find Data"></center>
    </div>
    {% endblock %}

    