{% extends "base.jinja2" %}
{% block title %}{{ super() }} - Help{% endblock %}
{% block pagetitle %}
	<h1>Help</h1>
{% endblock %}
{% block content %}
	<br/>
	{% for dataset in datasets %}
		{% if loop.first %}
			<ul>
		{% endif %}
	<li>
		<div class="filename"><a href="/data/{{ dataset.id }}">{{ dataset.title }}</a>&nbsp;-&nbsp;<a href="/data/{{ dataset.id }}/delete">(Delete)</a></div>
	</li>
	{% if loop.last %}
		</ul>
	{% endif %}
	{% else %}
		<p>You have no uploaded datasets. Files can be submitted at the <a href="/">upload page</a>.</p>
	{% endfor %}
{% endblock %}